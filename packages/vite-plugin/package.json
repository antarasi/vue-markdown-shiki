{
  "name": "vite-plugin-forvmsc",
  "version": "1.0.0",
  "description": "Use for vue-md-shiki-component to copy assets from node_modules",
  "private": false,
  "keywords": [
    "vue",
    "vue3",
    "vue-markdown-shiki",
    "vite",
    "plugin"
  ],
  "author": "toimc <brain@toimc.com>",
  "type": "module",
  "homepage": "",
  "license": "MIT",
  "types": "dist/plugin.d.ts",
  "main": "dist/plugin.cjs",
  "module": "dist/plugin.js",
  "exports": {
    ".": {
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsup lib/plugin.ts --format cjs,esm --dts --clean"
  },
  "peerDependencies": {
    "vue-markdown-shiki": "*"
  },
  "devDependencies": {
    "tsup": "^6.7.0"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "pkg-dir": "^7.0.0"
  }
}
