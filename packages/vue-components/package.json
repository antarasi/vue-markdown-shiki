{
  "name": "vue-markdown-shiki",
  "version": "1.4.2",
  "description": "A Vue 3 component library that provides integration with Shiki and Markdown-it. ",
  "keywords": [
    "vue",
    "vue3",
    "markdown",
    "shiki",
    "highlight",
    "markdown-it",
    "vue3-markdown-it"
  ],
  "author": "toimc <brain@toimc.com>",
  "homepage": "https://github.com/toimc/vue-markdown-shiki/blob/main/packages/vue-components/README.md",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "dev": "run-p build:watch start",
    "start": "vite",
    "build": "run-s clean type-check build-only",
    "build-only": "vite build",
    "build:watch": "vite build --watch",
    "type-check": "vue-tsc --noEmit",
    "clean": "rimraf dist/"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./style": "./dist/style.css",
    "./*": "./*"
  },
  "dependencies": {
    "@mdit-vue/plugin-headers": "^0.12.0",
    "@mdit-vue/plugin-sfc": "^0.12.0",
    "@mdit-vue/plugin-title": "^0.12.0",
    "@mdit-vue/plugin-toc": "^0.12.0",
    "@mdit-vue/shared": "^0.12.0",
    "@mdit-vue/types": "^0.12.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^2.0.2",
    "punycode": "^2.3.0",
    "shiki": "^0.14.1"
  },
  "devDependencies": {
    "@iconify-json/tabler": "^1.1.74",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/markdown-it-attrs": "^4.1.0",
    "@types/markdown-it-container": "^2.0.5",
    "@types/markdown-it-emoji": "^2.0.2",
    "@types/node": "^18.16.1",
    "lerna": "^6.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.3",
    "vite-plugin-dts": "^2.3.0",
    "vue": "^3.2.47",
    "vue-tsc": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/toimc/vue-markdown-shiki.git"
  },
  "gitHead": "a72e1a6a399bbe6eb03d5eee006af6b03cee47a6"
}
