{
  "name": "vue-markdown-shiki",
  "version": "2.0.0",
  "description": "A Vue 3 component library that provides integration with Shiki and Markdown-it. ",
  "keywords": [
    "vue",
    "vue3",
    "markdown",
    "shiki",
    "highlight",
    "markdown-it",
    "vue3-markdown-it"
  ],
  "author": "toimc <brain@toimc.com>",
  "homepage": "https://github.com/toimc/vue-markdown-shiki/blob/main/packages/vue-components/README.md",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "dev": "run-p build:watch start",
    "start": "vite",
    "build": "run-s clean type-check build-only",
    "build-only": "vite build",
    "build:watch": "vite build --watch",
    "type-check": "vue-tsc --noEmit",
    "clean": "rimraf dist/"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./style": "./dist/index.css",
    "./*": "./*"
  },
  "dependencies": {
    "@mdit-vue/plugin-headers": "^2.1.3",
    "@mdit-vue/plugin-sfc": "^2.1.3",
    "@mdit-vue/plugin-title": "^2.1.3",
    "@mdit-vue/plugin-toc": "^2.1.3",
    "@mdit-vue/shared": "^2.1.3",
    "@mdit-vue/types": "^2.1.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-container": "^4.0.0",
    "markdown-it-emoji": "^3.0.0",
    "punycode": "^2.3.1",
    "shiki": "^3.2.1"
  },
  "devDependencies": {
    "@iconify-json/tabler": "^1.2.17",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@types/markdown-it-attrs": "^4.1.3",
    "@types/markdown-it-container": "^2.0.10",
    "@types/markdown-it-emoji": "^3.0.1",
    "@types/node": "^22.13.16",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/tsconfig": "^0.7.0",
    "lerna": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.4",
    "vite-plugin-dts": "^4.5.3",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/toimc/vue-markdown-shiki.git"
  },
  "gitHead": "a72e1a6a399bbe6eb03d5eee006af6b03cee47a6"
}